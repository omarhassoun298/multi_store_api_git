{{ header }}{{ column_left }}
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      <nav aria-label="breadcrumb">
      <h1>{{ heading_title }}</h1>
        <ol class="breadcrumb">
          {% for breadcrumb in breadcrumbs %}
          <li class="breadcrumb-item">
            <a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a>
          </li>
          {% endfor %}
        </ol>
      </nav>
    </div>
  </div>
